k8s-secret-*.yaml
sealed-secret.yaml

# Local kubeconfig
kubeconfig
*.kubeconfig

# Generated files
k8s/generated/

# Coverage reports
excoveralls.json

# Package tarballs
shared-*.tar
command_service-*.tar
query_service-*.tar
client_service-*.tar
